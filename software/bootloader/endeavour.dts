/dts-v1/;

/ {
  #address-cells = <1>;
  #size-cells = <1>;
  model = "Endeavour";

  chosen {
      bootargs = "earlycon=endeavour_tty debug";
  };

  cpus {
    #address-cells = <1>;
    #size-cells = <0>;
    timebase-frequency = <1000000>;
    CPU0: cpu@0 {
      device_type = "cpu";
      reg = <0>;
      status = "okay";
      compatible = "riscv";
      riscv,isa = "rv32im";
      mmu-type = "riscv,sv32";
      clock-frequency = <92800000>;
    };
  };

  memory@80000000 {
      device_type = "memory";
      reg = <0x80000000 0x08000000>;
  };

  reserved-memory {
    #address-cells = <1>;
    #size-cells = <1>;
    ranges;

    display_reserved: framebuffer@80000000 {
      reg = <0x80000000 0x400000>;
    };
  };
};
