	component DDR_IO2 is
		port (
			inclock  : in    std_logic                    := 'X';             -- export
			outclock : in    std_logic                    := 'X';             -- export
			dout     : out   std_logic_vector(3 downto 0);                    -- export
			din      : in    std_logic_vector(3 downto 0) := (others => 'X'); -- export
			pad_io   : inout std_logic_vector(1 downto 0) := (others => 'X'); -- export
			oe       : in    std_logic_vector(1 downto 0) := (others => 'X')  -- export
		);
	end component DDR_IO2;

